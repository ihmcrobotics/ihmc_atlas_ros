<launch>
  <param name="use_sim_time" value="true"/>
  <arg name="use_local_build" default="false" />
  <arg name="ihmc_network_file" default="$(find ihmc_atlas)/configurations/IHMCNetworkParametersSim.ini" />
  <arg name="description_model" default="$(find ihmc_atlas)/urdf/atlas_unplugged_v5_no_hands.urdf" />
  <arg name="ihmc_model" default="ATLAS_UNPLUGGED_V5_NO_HANDS" />
  <arg name="starting_location" default="DEFAULT" />
  <arg name="tf_prefix" default="NONE" />
  <arg name="scs_args" default="" />

  <include file="$(find ihmc_ros_common)/launch/robot_description_common.launch">
    <arg name="robot_name" value="atlas" />
    <arg name="model_file" value="$(arg description_model)" />
  </include>

  <include file="$(find ihmc_atlas)/launch/common/ihmc_atlas_params_common.launch" />

  <node name="IHMCAtlasAPISimulatorDemo01" pkg="ihmc_ros_java_adapter" type="gradlew" args="runJava -Dus.ihmc.networkParameterFile=$(arg ihmc_network_file) -PuseLocal=$(arg use_local_build) -Pyaml=$(find ihmc_atlas)/configurations/demo01.yaml -PprogramArgs=-m,$(arg ihmc_model),-s,$(arg starting_location),--tfPrefix,$(arg tf_prefix),$(arg scs_args)" required="true" output="screen" cwd="node">
  </node>
</launch>
